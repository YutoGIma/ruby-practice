<h1>ユーザー一覧</h1>
<%= link_to "新規登録", new_user_path %>
<div>
    <% @users.each do |user| %>
        <div class="row">
            <p><%= user.id %></p>
            <p><%= user.name %></p>
            <p><%= user.email %></p>
            <%= link_to "詳細", user_path(user.id) %>
            <%= link_to "編集", edit_user_path(user.id) %>
            <%= button_to "削除", user_path(user.id), method: :delete %>
        </div>
    <% end %>
    <%= paginate @users %>
</div>